    {% extends 'common/app.html.twig' %}
    {% block header%}
    <h1>ProductSale</h1>
    {% endblock %}
    {% block content%}
    <table class="record_properties">
        <tbody>            <tr>
                <th>Id</th>                    <td>
                        {{ entity.id }}
                    </td>            </tr>            <tr>
                <th>Active</th>                    <td>
                        {{ entity.active }}
                    </td>            </tr>            <tr>
                <th>Startdate</th>                    <td>{{ entity.startDate|date('Y-m-d H:i:s') }}</td>            </tr>            <tr>
                <th>Enddate</th>                    <td>{{ entity.endDate|date('Y-m-d H:i:s') }}</td>            </tr>            <tr>
                <th>Price</th>                    <td>
                        {{ entity.price }}
                    </td>            </tr>        </tbody>
    </table>
        <div class="record-actions">
    <a class="button" href="{{ path('productsale') }}">
        Back to the list
    </a>
            <a class="button" href="{{ path('productsale_edit',
{ 'id': entity.id }) }}">
            Edit
        </a>
                <div style="float:left">
            {{ form(delete_form) }}
        </div>
        <div style="clear:both"></div>
    </div>    {% endblock %}
